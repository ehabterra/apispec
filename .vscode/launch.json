    "version": "0.2.0",
    "configurations": [
        {
            "name": "Generate OpenAPI for Gin Example",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "-d",
                "./testdata/gin",
                "-o",
                "./testdata/gin/openapi.yaml",
                "-title",
                "My Awesome API",
                "-openapi.version",
                "3.0.3",
                "-contact.name",
                "Ehab",
                "-contact.email",
                "support@example.com"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}